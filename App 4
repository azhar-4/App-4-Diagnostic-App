<script>
        var flu_count = 0;
        var covid_count = 0;
        
	var total = 0; 

        function fluAdd(){
            flu_count = flu_count + 1;
		//adds flu count by 1 
	    //flu count variable here is being added by one everytime coldAdd is called 
	    total++; 
            //everytime fluAdd is used, total is added by 1 
	<!-- function to count the number of answers that supports the user having the flu or covid -->
        }

        function covidAdd(){
            covid_count = covid_count + 1;
	    //covid count variable here is being added by one everytime covidAdd is called 
	    total++;
	    //everytime covidAdd is used, total is added by 1 
	<!-- function to count the number of answers that supports the user having the flu or covid -->
        }
print total; 
  </script>



